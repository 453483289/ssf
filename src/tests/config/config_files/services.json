{
    "ssf": {
        "services" : {
            "datagram_forwarder": { "enable": false },
            "datagram_listener": {
              "enable": false,
              "gateway_ports": true
            },
            "stream_forwarder": { "enable": false },
            "stream_listener": {
              "enable": false,
              "gateway_ports": true
            },
            "copy": { "enable": true },
            "shell": {
                "enable": true,
                "path": "/bin/custom_path",
                "args": "-custom args"
            },
            "socks": { "enable": false }
        }
    }
}
